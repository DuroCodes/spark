{
  "name": "@spark.ts/logger",
  "version": "1.3.4",
  "description": "a logger that extends the native console",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "tsup --watch --dts",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "build": "tsup && node scripts/mkjson.mjs dist/cjs dist/esm && tsup --dts-only --outDir dist",
    "publish": "npm run build && npm publish"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/durocodes/spark.git"
  },
  "keywords": [
    "logger",
    "console"
  ],
  "author": "durocodes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/durocodes/spark/issues"
  },
  "homepage": "https://github.com/durocodes/logger#readme",
  "devDependencies": {
    "@types/node": "18.8.4",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "eslint": "8.25.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "tsup": "6.2.3",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3"
  }
}
